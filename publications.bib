@misc{romiti_mathrmsun_2025,
	title = {{$\mathrm{SU}(N)$ lattice gauge theories with Physics-Informed Neural Networks}},
	url = {http://arxiv.org/abs/2510.26904},
	doi = {10.48550/arXiv.2510.26904},
	abstract = {We present an application of Physics-Informed Neural Networks ({PINNs}) to the study of \${\textbackslash}mathrm\{{SU}\}(N\_c)\$ lattice gauge theories. Our method enables the learning of eigenfunctions and eigenvalues at arbitrary gauge couplings, smoothly moving from the analytically known strong-coupling regime towards weaker couplings. By encoding the Schr{\textbackslash}"odinger equation and the symmetries of the eigenstates directly into the loss function, the network performs an unsupervised exploration of the spectrum. We validate the approach on the single-plaquette \${\textbackslash}mathrm\{U\}(1)\$ and \${\textbackslash}mathrm\{{SU}\}(2)\$ pure-gauge theories, showing that the {PINNs} successfully reproduce the hierarchy of energy levels and their corresponding wavefunctions.},
	number = {{arXiv}:2510.26904},
	publisher = {{arXiv}},
	author = {Romiti, Simone},
	urldate = {2025-11-07},
	date = {2025-10-30},
	eprinttype = {arxiv},
	eprint = {2510.26904 [hep-lat]},
	keywords = {High Energy Physics - Lattice},
	file = {Preprint PDF:/home/simone/Zotero/storage/PH4LYQ5U/Romiti - 2025 - \$mathrm SU(N) \$ lattice gauge theories with Physics-Informed Neural Networks.pdf:application/pdf;Snapshot:/home/simone/Zotero/storage/EVTW2UU3/2510.html:text/html},
}

